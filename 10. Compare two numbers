data segment
	msg1 db 'a>b$'
	msg2 db 'a=b$'
	msg3 db 'a<b$'
	
	a db 2h
	b db 3h
	
data ends

code segment
assume cs:code, ds:data
start:
	mov ax, data
	mov ds, ax
	
	mov ah, a
	mov al, b
	cmp ah,al
	jb label2
	je label3
	
label1:
	lea dx, msg1
	mov ah,09h
	int 21h
	jmp exit
label2:
	lea dx, msg2
	mov ah,09h
	int 21h
	jmp exit
label3:
	lea dx,msg3
	mov ah,09h
	int 21h

exit:	
	mov ah, 4ch
	int 21h

code ends
end start
