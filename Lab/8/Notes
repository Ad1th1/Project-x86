mov ah,01h 
int 21h
  character input with echo
  reads charater from standard input device and echoes to standard output
  al register contains ascii value
  
mov ah,02h
int 21h
  displays a character at standard output device
  dl contains ascii value
  
mov ah,06h
int 21h
  reads or writes a character to standard output device
  
mov ah,08h
int 21h
  reads character without echoing
  character is stored in al

mov ah,09h
int 21h
  displays string of characters
  
mov ah,0ah
int 21h
  reads string of characters from keyboard

  
