Transfer 5 words of data in data segment, where blocks are 
b).non-overlapping

data segment
array1 dw 1111h,2222h,3333h,4444h,5555h
array2 dw 5 dup(0)
count db 5
data ends

code segment
assume cs:code,ds:data
start:
mov ax, data
mov ds, ax

lea si, word ptr array1
lea di, word ptr array2

mov ch,00h
mov cl,count
next:
mov ax,[si]
mov di,ax
add si,2
add di,2
loop next

mov ah,4ch
int 21h

code ends
end start
