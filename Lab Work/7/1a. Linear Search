1. Linear search in an array of 10 unsigned 
i. Bytes

print macro msg
lea dx,msg
mov ah,09
int 21h
endm

data segment
arr db 11h,22h,33h,44h,55h,66h,77h,88h,99h,10h
ele db 11h
fnd db 'found$'
nfnd db 'not found$'
data ends

code segment
assume cs:code
start:

mov ax,data
mov ds,ax

lea si,arr
mov cx,10

back:
mov al,[si]
cmp ele,al
je F
inc si
loop back
print nfnd
jmp exit

F:
print fnd

exit:
mov ah,4ch
int 21h

code ends
end start
